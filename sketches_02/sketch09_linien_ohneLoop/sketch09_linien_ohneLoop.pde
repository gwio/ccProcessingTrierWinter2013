  line(50*3, 0, 50*3, height);

  line(50*4, 0, 50*4, height);

  line(50*5, 0, 50*5, height);

  line(50*6, 0, 50*6, height);

  line(50*7, 0, 50*7, height);

  line(50*8, 0, 50*8, height);

  line(50*9, 0, 50*9, height);
